/* pages/myperson/myperson.wxss */
.wrapper{
  width: 100%;
  display: block;
  background: #fff;
  height: 50px;
  padding:10px 0;
  border-bottom: solid 1px #eee;
}
.wrapper>view{
  height: 25px;
  width: 100%;
}
.wrapper>view>text{
  float: left;
  margin-left: 10px;
}
.wrapper>view>view{
  float: right;
  margin-right: 10px;
}
.wrapperBottom text{
  font-size: 12px;
  color:#8c8c8c;
  width: 30%;
  display: block;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
}
.wrapperBottom view{
  font-size: 12px;
  color: #8c8c8c;
}
.sickArr{
  height: 50px;
  line-height: 50px;
  background: #e2e2e2;
}
.sickArr text{
  margin-left: 20px;
}